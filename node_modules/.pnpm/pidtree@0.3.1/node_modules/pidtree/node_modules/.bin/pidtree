#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/pidtree@0.3.1/node_modules/pidtree/bin/node_modules:/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/pidtree@0.3.1/node_modules/pidtree/node_modules:/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/pidtree@0.3.1/node_modules:/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/pidtree@0.3.1/node_modules/pidtree/bin/node_modules:/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/pidtree@0.3.1/node_modules/pidtree/node_modules:/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/pidtree@0.3.1/node_modules:/home/tony/projects/learn/fullstackopen-submission/part11/test-render-deploy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pidtree.js" "$@"
else
  exec node  "$basedir/../../bin/pidtree.js" "$@"
fi
